{
  "name": "teselagen-react-components",
  "version": "24.3.41",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "types": "./typings.d.ts",
  "homepage": "http://teselagen.github.io/teselagen-react-components",
  "//module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "//build-watch": "nodemon --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "prepare": "in-publish && npm run build || not-in-publish",
    "postpublish": "yarn deploy",
    "predeploy": "yarn buildWithDemo",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d demo/dist",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "c": "yarn cypress open",
    "test": "yarn cypress run"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "cross-env": "^5.2.0",
    "deep-equal": "^1.0.1",
    "dom-scroll-into-view": "^1.2.1",
    "file-saver": "^1.3.8",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.11",
    "lodash.flatmap": "^4.5.0",
    "lru-memoize": "^1.0.2",
    "math-expression-evaluator": "^1.2.17",
    "mime-types": "^2.1.20",
    "moment": "^2.22.2",
    "papaparse": "^4.6.1",
    "pascal-case": "^2.0.1",
    "paths-js": "^0.4.7",
    "pluralize": "^7.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^5.1.0",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-color": "^2.14.1",
    "react-dropzone": "^4.2.12",
    "react-overflow-tooltip": "^2.0.1",
    "react-resize-detector": "^2.3.0",
    "react-rnd": "^8.0.2",
    "react-sortable-hoc": "^0.6.8",
    "react-values": "^0.2.4",
    "react-virtualized": "^9.20.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "uniqid": "^4.1.1"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3",
    "@blueprintjs/datetime": "^3",
    "apollo-cache-invalidation": "^0.0.3",
    "react-apollo": "^2.0.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.8.0",
    "redux-form": "^7.3.0",
    "tg-client-query-builder": "^2.6.0",
    "ve-range-utils": "^2.5.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.10.0",
    "@blueprintjs/datetime": "^3.5.0",
    "apollo-boost": "^0.1.15",
    "apollo-cache-invalidation": "^0.0.3",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chance": "^1.0.13",
    "cypress": "^3.1.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-teselagen": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-no-only-tests": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "husky": "^1.1.3",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "nwb": "^0.23.0",
    "prettier": "^1.14.3",
    "raw-loader": "^0.5.1",
    "react": "^16.6.3",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.6.3",
    "react-live": "^1.11.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "1.2.0",
    "react-table": "^6.8.0",
    "react-test-renderer": "^16.5.0",
    "react-transition-group": "^2.4.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "*",
    "sinon": "^4.4.9",
    "tg-client-query-builder": "^2.12.2",
    "typescript-tslint-plugin": "^0.2.1",
    "ve-range-utils": "^2.5.0",
    "why-did-you-update": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.js": [
      "eslint --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --rule 'import/order: 1' --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen",
    "blueprintjs",
    "multi-select",
    "side-by-side",
    "menu-bar",
    "timeline",
    "tree",
    "confirmation-dialog",
    "resizable-dialog"
  ]
}
